{
  "name": "color-color-app",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf public/*.{js,css,map}",
    "build:assets": "rollup -c",
    "watch:assets": "rollup -c -w",
    "dev": "run-s clean watch:*",
    "build": "run-s clean build:*",
    "start": "sirv public --host",
    "release:sentry": "./release-on-sentry.sh",
    "release": "run-s build release:*",
    "test": "jest"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com:saneef/color-color.git"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@sentry/cli": "^1.59.0",
    "autoprefixer": "^9.8.6",
    "babel-jest": "^26.6.3",
    "eslint": "^7.13.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "postcss-load-config": "^2.1.0",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.1",
    "@rollup/plugin-babel": "^5.2.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.29.7",
    "svelte-jest": "^0.3.1",
    "svelte-preprocess": "^4.5.2",
    "tailwindcss": "^1.9.6"
  },
  "dependencies": {
    "@borderless/base64": "^1.0.1",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@sentry/browser": "^5.27.4",
    "@xstate/fsm": "^1.5.1",
    "bezier-easing": "^2.1.0",
    "chroma-js": "^2.1.0",
    "core-js": "^3.7.0",
    "cssnano": "^4.1.10",
    "d3-scale": "^3.2.3",
    "d3-shape": "^2.0.0",
    "debounce-fn": "^4.0.0",
    "hsluv": "^0.1.0",
    "pako": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-nesting": "^7.0.1",
    "sirv-cli": "^1.0.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,svelte,css,md}": "prettier --write",
    "*.{js,svelte}": "eslint --cache --fix"
  }
}
